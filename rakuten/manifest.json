{
  "name": "Rakuten Simple Item Page",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": { "128": "img/logo_128.png" },
  "description": "This Extension can make Rakuten item page to be easy to watch and buy",

  "permissions": [
    "tabs","http://item.rakuten.cp.jp/*",
    "storage"
  ],

  // "browser_action":{
  //   "default_icon":"icon.png",
  //   "popup":"sw.html",
  //   "js":["js/jquery-1.8.3.js","js/change.js"]
  // },

  "options_page": "options.html",

  "content_scripts":[
  {
    "matches": ["http://item.rakuten.co.jp/*"],
    "css": ["css/easyRakuten.css", "css/jquery-ui-1.9.2.custom.css"],
    "js": ["js/jquery-1.8.3.js","js/jquery-ui-1.9.2.custom.js","js/easyRakuten.js"],
    "run_at": "document_start"
  }
  ]

}
